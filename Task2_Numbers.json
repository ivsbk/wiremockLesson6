{
	"request": {
		"urlPattern": "/orders/add", 
		"method": "POST",
		"bodyPatterns" : [{
			  "matches" : "<order>[0-9]{3}\\-[0-9]{2}\\-*[A-Z0-9a-z]{2}</order>"
		}]
	},
	"response":{
		"status": 200,
		"delayDistribution": {
			"type": "uniform",
			"lower":290,
			"upper":310
		}
	}
}