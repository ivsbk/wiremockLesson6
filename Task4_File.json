{
	"request": {
	"urlPath": "/template",
	"method": "GET",
	"queryParameters":{
		"nameOfFile":{
			"matches": "[1-3]"
		},
		"fileFormat":{
			"matches": "[A-Z0-9a-z]{1,}"
		}
	}
},
"response": {
	"status": 200,
	"fixedDelayMilliseconds": 1000,
	"bodyFileName":"file{{request.query.nameOfFile}}.{{request.query.fileFormat}}",
	"transformers": ["response-template"]
}
}
